{
  "name": "@runn/deploy-to-heroku-action",
  "version": "0.0.0",
  "private": true,
  "description": "Deploy a docker image to Heroku using Github Actions",
  "main": "lib/main.js",
  "scripts": {
    "clean": "pkg-clean",
    "build": "pkg-build && ncc build ./dist/index.js --out bin --minify",
    "tidy": "pkg-tidy",
    "lint": "pkg-lint",
    "test": "pkg-test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/runn/deploy-to-heroku-action.git"
  },
  "license": "UNLICENSED",
  "dependencies": {
    "@actions/core": "1.2.6",
    "@actions/exec": "1.0.4",
    "@actions/github": "4.0.0"
  },
  "devDependencies": {
    "@stayradiated/pkg": "5.5.0",
    "@types/node": "14.14.24",
    "@vercel/ncc": "0.27.0",
    "husky": "4.3.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run tidy && npm run build"
    }
  }
}
